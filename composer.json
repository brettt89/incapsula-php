{
    "name": "brettt89/incapsula-api-php",
    "description": "PHP library for Incapsula API",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^7"
    },
    "license": "BSD-3-Clause",
    "authors": [
      {
        "name": "Brett Tasker",
        "email": "brett.tasker@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "IncapsulaAPI\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": [
          "tests/"
      ]
    },
    "scripts": {
        "test": "phpunit",
        "coverage": [
          "phpunit --dump-xdebug-filter build/xdebug-filter.php",
          "phpunit --prepend build/xdebug-filter.php --coverage-clover=coverage.xml"
        ]
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    }
  }
